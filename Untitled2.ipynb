   "source": [
    "def try_except(func):\n",
    "    # try-except function. Usage: @try_except decorator\n",
    "    def handler(*args, **kwargs):\n",
    "        try:\n",
    "            func(*args, **kwargs)\n",
    "        except Exception as e:\n",
    "            print(e)\n",
    "\n",
    "    return handler"
   ]
  },
